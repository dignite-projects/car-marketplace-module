{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2017",
    "module": "es2020",
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@dignite/car-marketplace-dealer-platform": [
        "dist/admin-li"
      ],
      "@dignite/car-marketplace-dealer-platform/config": [
        "dist/car-marketplace-dealer-platform/config"
      ],
      
      "car-marketplace-dealer-platform": [
        "dist/car-marketplace-dealer-platform"
      ],
      "components": ["dist/components"]
    }
  },
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false
  }
}
